{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Proxy-Origin",
          "value": "linkproibido.com"
        },
        {
          "key": "Server",
          "value": "LinkProibidoServer"
        }
      ]
    }
  ]
}
